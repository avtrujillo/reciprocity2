<p id="notice"><%= notice %></p>

<p>
  <strong>User:</strong>
  <%= @profile_user.name %>
</p>

<% @profile_items.each do |profile_item| %>
<div>
	<p>
		<%= render partial: "profile_item_show", locals: {profile_item: profile_item} %>
	</p>
	<% if @profile_user == current_user %>
		<%= link_to "Edit", edit_profile_item_path(profile_item.id) %>
	<% end %>

</div>
<% end %>
<%= link_to "New Profile Item", new_profile_item_path %>

