<p id="notice"><%= notice %></p>

<h1>Profile Items</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Profile item category</th>
      <th>Privacy group</th>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile_item| %>
      <tr>
        <td><%= profile_item.user %></td>
        <td><%= profile_item.profile_item_category %></td>
        <td><%= profile_item.privacy_group %></td>
        <td><%= profile_item.data %></td>
        <td><%= link_to 'Show', profile_item %></td>
        <td><%= link_to 'Edit', edit_profile_item_path(profile_item) %></td>
        <td><%= link_to 'Destroy', profile_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile Item', new_profile_item_path %>
